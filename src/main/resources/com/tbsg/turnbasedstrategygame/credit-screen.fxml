<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox alignment="TOP_CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:id="root"
      fx:controller="com.tbsg.turnbasedstrategygame.controllers.CreditScreenController"
>
    <HBox alignment="TOP_LEFT">
        <Button HBox.hgrow="NEVER" onMouseClicked="#backToMenu" fx:id="buttonBack">
            <graphic>
                <ImageView fx:id="backButton" smooth="true">
                    <image>
                        <Image fx:id="image" url="@image/arrow.png"></Image>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </HBox>
    <!--    <Pane HBox.hgrow="ALWAYS"/>-->
    <Separator fx:id="separator"/>
    <Label text="Credits" fx:id="label" alignment="TOP_CENTER">
        <font>
            <Font name="System Bold" size="30.0"/>
        </font>
    </Label>
    <!--    <Pane HBox.hgrow="ALWAYS"/>-->
    <!--    </HBox>-->
    <VBox fx:id="list" alignment="TOP_CENTER"></VBox>
</VBox>
